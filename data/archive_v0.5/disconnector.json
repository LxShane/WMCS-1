  "surface_layer": {
    "definition": "A disconnector is a switching device with visible contacts, used to ensure that an electrical circuit is completely de-energized for service or maintenance.",
    "common_uses": [
      "Electrical distribution",
      "Industrial applications"
    ]
  },
  "deep_layer": {
    "mechanism": "Disconnectors provide a visible break in the circuit, ensuring safety during maintenance or repair by isolating equipment from the power source. They lack arc suppression capabilities and are therefore only operated when no current is flowing.",
    "origin": "Developed to provide a safe and reliable method of isolating electrical circuits."
  },
