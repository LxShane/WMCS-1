  "surface_layer": {
    "definition": "The IS 17017 Type 6 connector (often referred to as LEVDC for Light Electric Vehicle DC or Dash-6) is used for direct current (DC) fast charging of electric two-wheelers and electric three-wheelers, mainly within India.",
    "common_uses": [
      "Charging electric two-wheelers",
      "Charging electric three-wheelers"
    ]
  },
  "deep_layer": {
    "mechanism": "The battery management system on the electric vehicle negotiates the maximum current with the electric vehicle supply equipment via dedicated pins in the Type 6 connector using CAN bus communication protocol after the initial control pilot handshake.",
    "origin": "Derived from the international CHAdeMO ePTW standard, specifically designed for low-voltage DC fast charging applications."
  },
