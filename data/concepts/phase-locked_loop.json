  "surface_layer": {
    "definition": "A phase-locked loop (PLL) is a control system that generates an output signal whose phase is fixed relative to the phase of an input signal.",
    "common_uses": [
      "clock synchronization",
      "demodulation",
      "frequency synthesis"
    ]
  },
  "deep_layer": {
    "mechanism": "Locks the output frequency to the input frequency using negative feedback.",
    "origin": "Since 1969, a single integrated circuit can provide a complete PLL building block."
  },
